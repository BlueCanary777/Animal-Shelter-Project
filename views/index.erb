<% @animals.each do |animal| %>
  <div class = "current-animals">
    <p>Name: <%= animal.name() %></p>
    <p>Admission Date: <%= animal.admission_date %></p>
    <p>Adoptability: <%= animal.adoptable %></p>
    <% if animal.owner_id %>
    <p>Owner: <%= animal.owner.pretty_name %></p>
    <% end %>
    <p><a class="body-link" href="/animals/<%= animal.id %>/edit">Edit Details</a></p>
    <hr>
  </div>
<% end %>
